{
  "name": "codehelp-backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "dev-local": "dotenv -e .env.local nodemon src/index.ts",
    "start": "node dist/src/index.js",
    "async-entities": "dotenv -e .env.development cross-var -- typeorm-model-generator -h %DB_HOST% -d %DB% -p %DB_PORT% -u %DB_USER% -x %DB_PWD% -e postgres -o src/db/entities --noConfig -a --strictMode ?",
    "migration:create": "typeorm migration:create src/db/migrations/Template",
    "migration:up": "typeorm-ts-node-commonjs migration:run -d src/db/dataSource.ts",
    "migration:down": "typeorm-ts-node-commonjs migration:revert -d src/db/dataSource.ts",
    "dev-migration:up": "dotenv -e .env.development -- typeorm-ts-node-commonjs migration:run -d src/db/dataSource.ts",
    "dev-migration:down": "dotenv -e .env.development -- typeorm-ts-node-commonjs migration:revert -d src/db/dataSource.ts",
    "build": "tsc && tsc-alias",
    "test": "dotenv -e .env.test jest --detectOpenHandles"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.658.1",
    "@aws-sdk/lib-storage": "^3.658.1",
    "@prisma/client": "^6.13.0",
    "@types/node": "^22.0.2",
    "@types/pg": "^8.11.6",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cross-var": "^1.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validation": "^4.1.1",
    "iso-3166-1-alpha-2": "^1.0.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-pg-migrate": "^7.6.0",
    "nodemon": "^3.1.3",
    "pg": "^8.12.0",
    "socket.io": "^4.7.5",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "typeorm-model-generator": "^0.4.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@types/bcrypt": "^5.0.2",
    "@types/better-sqlite3": "^7.6.11",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.3",
    "@types/express-serve-static-core": "^5.0.7",
    "@types/iso-3166-1-alpha-2": "^1.0.2",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^2.0.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "better-sqlite3": "^11.3.0",
    "dotenv-cli": "^7.4.2",
    "globals": "^15.6.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.1",
    "ultracite": "5.1.2"
  },
  "packageManager": "yarn@4.7.0"
}
